		"php": ">= 5.5", 
		"noresources/ns-php-core": "^2.3"
	}, 
	"require-dev": {
		"phpunit/phpunit": ">= 5.0"
	}, 
	"extra": {
		"branch-alias": {
			"dev-master": "1.2.x-dev"
		}
	}
}